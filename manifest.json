{
    "name": "auto-zoom",
    "description": "A chrome extension developed for automatically opening zoom links!",
    "version": "1.0",
    "manifest_version": 3,
    "chrome_url_overrides": { "newtab": "/landing-page/newTab.html" },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
          }
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    }
  }
